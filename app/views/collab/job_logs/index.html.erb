<div class="py-6">
  <%= render(Collab::TitleComponent.new) do |t| %>
    <%= t(".title") %>
  <% end %>
  <div class="px-4 sm:px-6 md:px-8">
    <%= render(Collab::TableComponent.new) do |tc| %>
      <%
        tc.with_head_td { t("activerecord.attributes.job_log.job_name") }
        tc.with_head_td { t("activerecord.attributes.job_log.job_id") }
        tc.with_head_td { t("activerecord.attributes.job_log.status") }
        tc.with_head_td { t("activerecord.attributes.job_log.logs") }
        tc.with_head_td { t("activerecord.attributes.job_log.updated_at") }
        tc.with_head_td { t("activerecord.attributes.job_log.duration") }
      %>
      <% @job_logs.each do |job_log| %>
        <% tc.with_row do |r| %>
          <%
            r.with_row_td { job_log.job_name }
            r.with_row_td { job_log.job_id }
            r.with_row_td { job_log.status }
            r.with_row_td { job_log.logs }
            r.with_row_td { l(job_log.updated_at) }
            r.with_row_td { job_log_duration(job_log: job_log) }
          %>
        <% end %>
      <% end %>
    <% end %>
    <%= render(PaginationComponent.new(pagy: @pagy)) %>
  </div>
</div>
